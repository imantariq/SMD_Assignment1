<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".page10"
    android:background="@color/black"
    android:orientation="vertical">
    <LinearLayout
            android:layout_width="400dp"
            android:layout_height="90dp"
            android:layout_marginTop="30dp">
            <ImageView
                android:layout_width="80dp"
                android:layout_marginTop="20dp"
                android:layout_height="35dp"
                android:layout_marginLeft="-10dp"
                android:src="@drawable/arrow">
            </ImageView>
            <TextView
                android:layout_width="250dp"
                android:layout_height="70dp"
                android:text="Upload Music"
                android:layout_marginTop="10dp"
                android:layout_marginRight="90dp"
                android:layout_marginLeft="-10dp"
                android:textColor="@color/white"
                android:textSize="35dp"></TextView>
           <Button
               android:id="@+id/next"
               android:layout_width="90dp"
               android:layout_height="50dp"
               android:text="Next"
               android:layout_marginTop="5dp"
               android:textSize="20dp"
               android:layout_marginStart="-80dp"
               android:textColor="#FF9800"
               android:backgroundTint="@color/black"></Button>
        </LinearLayout>
        <LinearLayout
            android:layout_width="400dp"
            android:layout_height="200dp"
            android:layout_marginTop="-20dp">
            <ImageView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:src="@drawable/upload"></ImageView>
        </LinearLayout>

        <LinearLayout
            android:layout_width="400dp"
            android:layout_height="15dp">
        </LinearLayout>
        <EditText
          android:layout_marginStart="10dp"
            android:id="@+id/pn"
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:textColor="#FFC107"
            android:layout_marginLeft="10dp"
            android:text="Title"
            android:textSize="20dp"
            android:padding="10dp"
            android:background="@drawable/white"
            />

        <LinearLayout
            android:layout_width="400dp"
            android:layout_height="15dp">
        </LinearLayout>
        <EditText
            android:layout_marginStart="10dp"
            android:id="@+id/gen"
            android:layout_width="400dp"
            android:layout_height="50dp"
            android:textColor="#FFC107"
            android:text="Genre"
            android:textSize="20dp"
            android:padding="10dp"
            android:background="@drawable/white"
            />
    <LinearLayout
        android:layout_width="400dp"
        android:layout_height="15dp">
    </LinearLayout>
    <EditText
        android:layout_marginStart="10dp"
        android:id="@+id/dep"
        android:layout_width="400dp"
        android:layout_height="80dp"
        android:textColor="#FFC107"
        android:text="Description"
        android:textSize="20dp"
        android:padding="10dp"
        android:background="@drawable/white"
        />
        <LinearLayout
            android:layout_width="400dp"
            android:layout_height="60dp">
            <Button
                android:id="@+id/button1"
                android:layout_width="150dp"
                android:layout_marginTop="10dp"
                android:layout_height="60dp"
                android:layout_marginLeft="20dp"
                android:text="Upload"/>
            <Button
                android:id="@+id/button2"
                android:layout_width="150dp"
                android:layout_marginTop="10dp"
                android:layout_height="60dp"
                android:layout_marginStart="90dp"
                android:text="Record"
                android:textSize="15dp"
                android:backgroundTint="@color/black"/>
        </LinearLayout>


        <RelativeLayout
            android:layout_width="90dp"
            android:layout_height="100dp"
            android:layout_marginTop="46dp"
            android:background="@drawable/grey">

        </RelativeLayout>

        <RelativeLayout
            android:layout_width="230dp"
            android:layout_height="100dp"
            android:layout_marginLeft="190dp"
            android:layout_marginTop="-100dp"
            android:background="@drawable/grey">

        </RelativeLayout>

        <RelativeLayout
            android:layout_width="80dp"
            android:layout_height="40dp"
            android:layout_marginTop="-90dp"
            android:layout_marginLeft="4dp">

            <ImageView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:clickable="true"
                android:src="@drawable/heart"
                tools:ignore="TouchTargetSizeCheck,TouchTargetSizeCheck,SpeakableTextPresentCheck,SpeakableTextPresentCheck"></ImageView>
        </RelativeLayout>


        <RelativeLayout
            android:layout_width="80dp"
            android:layout_height="40dp"
            android:layout_marginTop="-40dp"
            android:layout_marginLeft="100dp">

            <ImageView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:clickable="true"
                android:src="@drawable/plus"
                tools:ignore="SpeakableTextPresentCheck,SpeakableTextPresentCheck,TouchTargetSizeCheck,TouchTargetSizeCheck"
                ></ImageView>
        </RelativeLayout>

        <RelativeLayout
            android:layout_width="80dp"
            android:layout_height="40dp"
            android:layout_marginTop="-40dp"
            android:layout_marginLeft="200dp">

            <ImageView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:clickable="true"
                android:src="@drawable/search"
                tools:ignore="SpeakableTextPresentCheck,SpeakableTextPresentCheck,TouchTargetSizeCheck,TouchTargetSizeCheck"></ImageView>
        </RelativeLayout>

        <RelativeLayout
            android:layout_width="80dp"
            android:layout_height="40dp"
            android:layout_marginTop="-40dp"
            android:layout_marginLeft="300dp">

            <ImageView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:clickable="true"
                android:src="@drawable/listen_later"
                tools:ignore="TouchTargetSizeCheck,TouchTargetSizeCheck,SpeakableTextPresentCheck,SpeakableTextPresentCheck"></ImageView>
        </RelativeLayout>
        <LinearLayout
            android:layout_width="400dp"
            android:layout_height="30dp"
            android:layout_marginTop="10dp">
            <TextView
                android:layout_width="50dp"
                android:layout_height="40dp"
                android:layout_marginLeft="20dp"
                android:text="Liked"
                android:textSize="20dp"
                android:textColor="@color/white"></TextView>

            <TextView
                android:layout_width="50dp"
                android:layout_height="40dp"
                android:layout_marginLeft="50dp"
                android:text="Add"
                android:textSize="20dp"
                android:textColor="@color/white"></TextView>

            <TextView
                android:layout_width="65dp"
                android:layout_height="40dp"
                android:layout_marginLeft="40dp"
                android:text="Search"
                android:textSize="20dp"
                android:textColor="@color/white"></TextView>
            <TextView
                android:layout_width="110dp"
                android:layout_height="40dp"
                android:layout_marginLeft="25dp"
                android:text="ListenLater"
                android:textSize="20dp"
                android:textColor="@color/white"></TextView>
        </LinearLayout>
</LinearLayout>