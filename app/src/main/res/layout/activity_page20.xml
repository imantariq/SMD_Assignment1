<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".page20"
    android:background="@color/black"
    android:orientation="vertical">
<LinearLayout
    android:layout_width="400dp"
    android:layout_height="90dp"
    android:layout_marginTop="30dp">
    <ImageView
        android:layout_width="80dp"
        android:layout_marginTop="10dp"
        android:layout_height="35dp"
        android:layout_marginLeft="-10dp"
        android:src="@drawable/arrow">
    </ImageView>
    <TextView
        android:layout_width="320dp"
        android:layout_height="55dp"
        android:text="Liked"
        android:textAlignment="center"
        android:textColor="@color/white"
        android:layout_marginTop="-10dp"
        android:textSize="35dp"></TextView>
</LinearLayout>
<TextView
    android:layout_width="240dp"
    android:layout_height="40dp"
    android:layout_marginTop="-30dp"
    android:layout_marginLeft="20dp"
    android:text="2 options/Right Selected"
    android:textColor="#504D4A"
    android:textSize="20dp"></TextView>
<LinearLayout
    android:layout_width="400dp"
    android:layout_height="50dp">
    <ImageView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:src="@drawable/selectbar2"></ImageView>
</LinearLayout>
<RelativeLayout
    android:layout_width="400dp"
    android:layout_height="100dp"
    android:layout_marginTop="40dp"
    android:layout_marginLeft="5dp">
    <ImageView
        android:layout_width="match_parent"
        android:layout_height="100dp"
        android:layout_marginLeft="-260dp"
        android:src="@drawable/playlist"></ImageView>
</RelativeLayout>
<RelativeLayout
    android:layout_width="110dp"
    android:layout_height="50dp"
    android:layout_marginLeft="140dp"
    android:layout_marginTop="-100dp">
    <TextView
        android:layout_width="110dp"
        android:layout_height="50dp"
        android:text="Playlist 1"
        android:textSize="18dp"
        android:textColor="@color/white"></TextView>
</RelativeLayout>
<RelativeLayout
    android:layout_width="110dp"
    android:layout_height="50dp"
    android:layout_marginLeft="140dp"
    android:layout_marginTop="-15dp">
    <TextView
        android:layout_width="110dp"
        android:layout_height="50dp"
        android:text="Artist"
        android:textSize="20dp"
        android:textColor="#FF9800"></TextView>
</RelativeLayout>
<RelativeLayout
    android:layout_width="110dp"
    android:layout_height="40dp"
    android:layout_marginLeft="300dp"
    android:layout_marginTop="-70dp">
    <ImageView
        android:layout_width="100dp"
        android:layout_height="50dp"
        android:src="@drawable/dots"></ImageView>
    <TextView
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:text="Track:8"
        android:textColor="@color/white"
        android:layout_marginTop="10dp"></TextView>
</RelativeLayout>
<LinearLayout
    android:layout_width="400dp"
    android:layout_height="30dp"></LinearLayout>

<RelativeLayout
    android:layout_width="400dp"
    android:layout_height="100dp"
    android:layout_marginTop="40dp"
    android:layout_marginLeft="5dp">
    <ImageView
        android:layout_width="match_parent"
        android:layout_height="100dp"
        android:layout_marginLeft="-260dp"
        android:src="@drawable/playlist"></ImageView>
</RelativeLayout>
<RelativeLayout
    android:layout_width="110dp"
    android:layout_height="50dp"
    android:layout_marginLeft="140dp"
    android:layout_marginTop="-100dp">
    <TextView
        android:layout_width="110dp"
        android:layout_height="50dp"
        android:text="Playlist 2"
        android:textSize="18dp"
        android:textColor="@color/white"></TextView>
</RelativeLayout>
<RelativeLayout
    android:layout_width="110dp"
    android:layout_height="50dp"
    android:layout_marginLeft="140dp"
    android:layout_marginTop="-15dp">
    <TextView
        android:layout_width="110dp"
        android:layout_height="50dp"
        android:text="Artist"
        android:textSize="20dp"
        android:textColor="#FF9800"></TextView>
</RelativeLayout>
<RelativeLayout
    android:layout_width="110dp"
    android:layout_height="40dp"
    android:layout_marginLeft="300dp"
    android:layout_marginTop="-70dp">
    <ImageView
        android:layout_width="100dp"
        android:layout_height="50dp"
        android:src="@drawable/dots"></ImageView>
    <TextView
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:text="Track:3"
        android:textColor="@color/white"
        android:layout_marginTop="10dp"></TextView>
</RelativeLayout>


<RelativeLayout
    android:layout_width="420dp"
    android:layout_height="100dp"
    android:layout_marginTop="165dp"
    android:background="@drawable/grey">

</RelativeLayout>


<RelativeLayout
    android:layout_width="80dp"
    android:layout_height="40dp"
    android:layout_marginTop="-90dp"
    android:layout_marginLeft="4dp">

    <ImageView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:clickable="true"
        android:src="@drawable/heart"
        tools:ignore="TouchTargetSizeCheck,TouchTargetSizeCheck,SpeakableTextPresentCheck,SpeakableTextPresentCheck"></ImageView>
</RelativeLayout>


<RelativeLayout
    android:layout_width="80dp"
    android:layout_height="40dp"
    android:layout_marginTop="-40dp"
    android:layout_marginLeft="100dp">

    <ImageView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:clickable="true"
        android:src="@drawable/plus"
        tools:ignore="SpeakableTextPresentCheck,SpeakableTextPresentCheck,TouchTargetSizeCheck,TouchTargetSizeCheck"
        ></ImageView>
</RelativeLayout>

<RelativeLayout
    android:layout_width="80dp"
    android:layout_height="40dp"
    android:layout_marginTop="-40dp"
    android:layout_marginLeft="200dp">

    <ImageView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:clickable="true"
        android:src="@drawable/search"
        tools:ignore="SpeakableTextPresentCheck,SpeakableTextPresentCheck,TouchTargetSizeCheck,TouchTargetSizeCheck"></ImageView>
</RelativeLayout>

<RelativeLayout
    android:layout_width="80dp"
    android:layout_height="40dp"
    android:layout_marginTop="-40dp"
    android:layout_marginLeft="310dp">

    <ImageView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:clickable="true"
        android:src="@drawable/listen_later"
        tools:ignore="TouchTargetSizeCheck,TouchTargetSizeCheck,SpeakableTextPresentCheck,SpeakableTextPresentCheck"></ImageView>
</RelativeLayout>
<LinearLayout
    android:layout_width="400dp"
    android:layout_height="30dp"
    android:layout_marginTop="10dp">
    <TextView
        android:layout_width="50dp"
        android:layout_height="40dp"
        android:layout_marginLeft="20dp"
        android:text="Liked"
        android:textSize="20dp"
        android:textColor="@color/white"></TextView>

    <TextView
        android:layout_width="50dp"
        android:layout_height="40dp"
        android:layout_marginLeft="50dp"
        android:text="Add"
        android:textSize="20dp"
        android:textColor="@color/white"></TextView>

    <TextView
        android:layout_width="65dp"
        android:layout_height="40dp"
        android:layout_marginLeft="40dp"
        android:text="Search"
        android:textSize="20dp"
        android:textColor="@color/white"></TextView>
    <TextView
        android:layout_width="110dp"
        android:layout_height="40dp"
        android:layout_marginLeft="25dp"
        android:text="ListenLater"
        android:textSize="20dp"
        android:textColor="@color/white"></TextView>
</LinearLayout>
</LinearLayout>
